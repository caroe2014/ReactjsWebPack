    "webpack-cli": "^2.0.13",
    "webpack-dev-server": "^3.1.1",
    "webpack-simple-progress-plugin": "^0.0.4",
    "yarn": "^1.5.1"
  },
  "nodemonConfig": {
    "ignore": [
      "*.test.js",
      "/web/client/**",
      "/public",
      "webpack-assets.json"
    ]
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      },
      "agilysys.lib/**/*.js?(x)": {
        "branches": 60,
        "functions": 60,
        "lines": 60,
        "statements": 60
      },
      "agilysys.lib/*.js?(x)": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      },
      "api/**/*.js?(x)": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      },
      "web/": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      },
      "web/client/app/components/**/*.js?(x)": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      },
      "web/client/app/modules/**/*.js?(x)": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      },
      "web/client/app/utils/*.js?(x)": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      },
      "web/client/entries/*.js?(x)": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    },
    "moduleFileExtensions": [
      "",
      "jsx",
      "js",
      "json",
      "css"
    ],
    "testMatch": [
      "**/__tests__/**/*.js?(x)",
      "**/?(*.)+(test).js?(x)"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.jsx$": "babel-jest"
    },
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/FileStub.js",
      "^.+\\.css$": "<rootDir>/__mocks__/CSSStub.js",
      "@/(.*)$": "<rootDir>/$1",
      "^api/((?!\\${.*$).*)": "<rootDir>/api/$1",
      "^web/(.*)$": "<rootDir>/web/$1",
      "^node_scripts/(.*)$": "<rootDir>/node_scripts/$1",
      "^__mocks__/(.*)$": "<rootDir>/__mocks__/$1",
      "^app.config$": "<rootDir>/app.config.js",
      "^env.config$": "<rootDir>/env.config.js",
      "^agilysys.lib$": "<rootDir>//agilysys.lib/index.js",
      "^styled-components": "<rootDir>/node_modules/styled-components"
    },
    "modulePathIgnorePatterns": [
      "<rootDir>/coverage"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/(build|docs|coverage|node_scripts)/",
      "<rootDir>/node_modules"
    ],
    "testEnvironment": "jsdom"
  }
}
