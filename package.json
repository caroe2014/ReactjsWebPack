{
  "name": "webapp",
  "version": "1.0.0",
  "description": "testing ground for setting up a new webapp with proper configs for webpack, babel, eslint, etc...",
  "main": "index.js",
  "author": "Zach Mayer",
  "license": "MIT",
  "scripts": {
    "postinstall": "echo 'Project Node Version: ' && node -v",
    "make": "cross-env NODE_ENV=production node ./node_scripts/make.js | ./node_modules/.bin/bunyan -o short",
    "make:dev": "cross-env NODE_ENV=development node ./node_scripts/make.js | ./node_modules/.bin/bunyan -o short",
    "clean": "echo 'clean is a noop placeholder for maven'",
    "test": "node ./node_scripts/test.js",
    "test:coverage": "node ./node_scripts/test.js --coverage",
    "test:watch": "node ./node_scripts/test.js --watch",
    "test:update": "yarn test -u",
    "clean:dev": "cross-env NODE_ENV=development node ./node_scripts/clean.js | ./node_modules/.bin/bunyan -o short",
    "build": "yarn make && webpack -p --config webpack-config.babel.js | ./node_modules/.bin/bunyan -o short",
    "lint": "yarn make && eslint \"web/**\"",
    "start:webpack": "webpack-dev-server --config webpack-config.babel.js | ./node_modules/.bin/bunyan -o short",
    "start:hapi": "nodemon --harmony ./node_scripts/start.js | ./node_modules/.bin/bunyan -o short",
    "prestart:dev": "./node_modules/check-node-version/bin.js --node '>= 8.11'",
    "start:dev": "yarn make:dev && yarn start:webpack & yarn start:hapi",
    "start:local": "yarn build && yarn start:hapi",
    "start:monit": "yarn start && yarn monit",
    "start:docker": "yarn build && pm2-docker pm2config.json",
    "start": "yarn build && pm2 startOrRestart pm2config.json",
    "stop": "pm2 kill",
    "restart": "pm2 restart kiosk-desktop",
    "status": "pm2 status kiosk-desktop",
    "monit": "pm2 logs --raw --lines 0 | ./node_modules/.bin/bunyan -o short",
    "storybook": "start-storybook -p 6006 -s ./public/static",
    "build-storybook": "build-storybook"
  },
